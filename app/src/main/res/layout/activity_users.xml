<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.cuong.alumni.Users">

    <EditText
        android:id="@+id/edFindUser09"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:hint="Nhập tên user cần tìm"
        android:textAlignment="center"
        android:layout_centerHorizontal="true" />
    <Button
        android:id="@+id/btnOk09"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OK"
        android:layout_below="@id/edFindUser09"
        android:layout_centerHorizontal="true"
        android:textSize="15dp"
        android:background="#FFFF5940"/>

    <ListView
        android:id="@+id/lv09"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_below="@+id/btnOk09"
        android:layout_marginTop="30dp">

    </ListView>

</RelativeLayout>
